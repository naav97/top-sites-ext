{
  "manifest_version": 3,
  "name": "Top Visited Sites",
  "version": "1.0",
  "description": "Displays the user's top visited sites.",
  "permissions": [
    "topSites"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
